{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "WebWorker"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": ["./worker-configuration.d.ts", "@cloudflare/workers-types"],
    "noEmit": true,
    "isolatedModulesd": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["src"]
}